{
  "name": "@guidepup/playwright-voiceover",
  "version": "0.1.0",
  "description": "Guidepup VoiceOver and Playwright example",
  "author": "Craig Morten <craig.morten@hotmail.co.uk>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "npx playwright install chrome chromium firefox msedge webkit",
    "pretest": "yarn --cwd ../.. compile",
    "test:chrome": "playwright test --browser chrome chrome/",
    "test:chromium": "playwright test --browser chromium chromium/",
    "test:firefox": "playwright test --browser firefox firefox/",
    "test:msedge": "playwright test --browser msedge msedge/",
    "test:webkit": "playwright test --browser webkit webkit/"
  },
  "devDependencies": {
    "@playwright/test": "^1.17.1",
    "@types/node": "^17.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
